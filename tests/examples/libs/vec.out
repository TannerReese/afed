(v3 // 1.2) + (v1 // 0.8) = `V[11, 10, -1]`,

# Dot product
v1 * (v2 + v3) = `15.214297924297924`,
V[3/4, 2/3, -8/9] * V[1, -2, 10] = `-341 / 36`,
v1 - (v1 * v2) * v2 = `V[1, -141.92045043956045, 162.33421507064364]`,

# Vector functions
mag v1 = `2.7024534108601803`, vec.mag2 v2 = `118.63687844897959`,
dims v3 = `3`,

